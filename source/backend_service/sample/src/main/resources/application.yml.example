oss:
  enable: true
  provider: minio
  endpoint: [YOUR_MINIO_ENDPOINT]
  access-key: [YOUR_MINIO_ACCESS_KEY]
  secret-key: [YOUR_MINIO_SECRET_KEY]
  bucket: dji-cloud
  expire: 3600
  region: us-east-1
  object-dir-prefix: wayline

logging:
  level:
    com.dji: debug
  file:
    name: logs/cloud-api-sample.log

ntp:
  server:
    host: ntp.aliyun.com

# To create a license for an application: https://developer.dji.com/user/apps/#all
cloud-api:
  app:
    id: [YOUR_DJI_APP_ID]
    key: [YOUR_DJI_APP_KEY]
    license: [YOUR_DJI_APP_LICENSE]

livestream:
  url:
    gb28181:
      serverIP: [YOUR_GB28181_SERVER_IP]
      serverPort: 5060
      serverID: [YOUR_GB28181_SERVER_ID]
      agentID: [YOUR_GB28181_AGENT_ID]
      agentPassword: [YOUR_GB28181_PASSWORD]
      localPort: 8116
      channel: [YOUR_GB28181_CHANNEL]
    # It is recommended to use a program to create Token. https://github.com/AgoraIO/Tools/blob/master/DynamicKey/AgoraDynamicKey/java/src/main/java/io/agora/media/RtcTokenBuilder2.java
    # agora:
    #   channel: Please enter the agora channel.
    #   token: Please enter the agora temporary token.
    #   uid:  654321

    # RTMP  Note: This IP is the address of the streaming server. If you want to see livestream on web page, you need to convert the RTMP stream to WebRTC stream.
    rtmp:
      url: rtmp://[YOUR_RTMP_SERVER]:1935/rtp/  # Example: 'rtmp://192.168.1.1/live/'
    rtsp:
      username: [YOUR_RTSP_USERNAME]
      password: [YOUR_RTSP_PASSWORD]
      port: 8554

    # GB28181 Note:If you don't know what these parameters mean, you can go to Pilot2 and select the GB28181 page in the cloud platform. Where the parameters same as these parameters.


    # Webrtc: Only supports using whip standard
    whip:
      url: http://[YOUR_WHIP_SERVER]:8088/rtc/v1/whip/?app=live&stream=
      # url: http://[YOUR_WHIP_SERVER]:8088/index/api/webrtc?app=rtp&type=play&stream=