server:
  port: 6789
spring:
  main:
    allow-bean-definition-overriding: true
  application:
    name: cloud-api-sample
  datasource:
    druid:
      type: com.alibaba.druid.pool.DruidDataSource
      driver-class-name: com.mysql.cj.jdbc.Driver
      url: jdbc:mysql://[YOUR_MYSQL_HOST]:3306/cloud_sample?useSSL=false&allowPublicKeyRetrieval=true
      username: [YOUR_MYSQL_USERNAME]
      password: [YOUR_MYSQL_PASSWORD]
      initial-size: 10
      min-idle: 10
      max-active: 20
      max-wait: 60000

  redis:
    host: [YOUR_REDIS_HOST]
    port: 6379
    database: 1
    # username:  # if you enable
    password: [YOUR_REDIS_PASSWORD]
    lettuce:
      pool:
        max-active: 8
        max-idle: 8
        min-idle: 0

  servlet:
    multipart:
      max-file-size: 2GB
      max-request-size: 2GB

jwt:
  issuer: DJI
  subject: CloudApiSample
  secret: [YOUR_JWT_SECRET]
  age: 86400

mqtt:
  # @see com.dji.sample.component.mqtt.model.MqttUseEnum
  # BASIC parameters are required.
  BASIC:
    protocol: MQTT # @see com.dji.sample.component.mqtt.model.MqttProtocolEnum
    host: [YOUR_MQTT_HOST]
    port: 1883
    username: [YOUR_MQTT_USERNAME]
    password: [YOUR_MQTT_PASSWORD]
    client-id: [YOUR_CLIENT_ID]
    # If the protocol is ws/wss, this value is required.
    path: /mqtt1
  DRC:
    protocol: WS # @see com.dji.sample.component.mqtt.model.MqttProtocolEnum
    host: [YOUR_MQTT_HOST]
    port: 8083
    path: /mqtt
    username: [YOUR_MQTT_USERNAME]
    password: [YOUR_MQTT_PASSWORD]

cloud-sdk:
  mqtt:
    # Topics that need to be subscribed when initially connecting to mqtt, multiple topics are divided by ",".
    inbound-topic: sys/product/+/status,thing/product/+/requests

url:
  manage:
    prefix: manage
    version: /api/v1
  map:
    prefix: map
    version: /api/v1
  media:
    prefix: media
    version: /api/v1
  wayline:
    prefix: wayline
    version: /api/v1
  storage:
    prefix: storage
    version: /api/v1
  control:
    prefix: control
    version: /api/v1

